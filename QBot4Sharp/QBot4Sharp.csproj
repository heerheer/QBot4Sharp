<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>1.0.4-alpha</PackageVersion>
        <Title>QBot4Sharp</Title>
        <Authors>Heer Kaisair</Authors>
        <Description>A QBot SDK for .Net Developer</Description>
        <PackageProjectUrl>https://github.com/heerheer/QBot4Sharp</PackageProjectUrl>
        <RepositoryUrl>https://github.com/heerheer/QBot4Sharp</RepositoryUrl>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Websocket.Client" Version="4.4.43" />
    </ItemGroup>

</Project>
